  "version": "1.0.2",
  "homepage": "https://ST4RFKR.github.io/ui-kit",
  "type": "module",
  "exports": {
    "./components": {
      "import": "./dist/components/index.esm.js",
      "require": "./dist/components/index.js"
    },
    "./icons": {
      "import": "./dist/icons/index.esm.js",
      "require": "./dist/icons/index.js"
    },
    "./types": {
      "import": "./dist/types/components/index.d.ts",
      "require": "./dist/types/icons/index.d.ts"
    },
    "./components/styles": "./dist/components/index.css"
  },
  "files": [
    "dist/**/*"
  ],
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "rename-icons": "node src/core/scripts/rename-icons.cjs",
    "generate-icons": "pnpm run rename-icons && npx @svgr/cli --icon --out-dir src/icons --jsx-runtime automatic --typescript --ref --memo --replace-attr-values \"#000=currentcolor\"  --replace-attr-values \"#fff=currentcolor\" -- src/core/assets/icons/svg && node scripts/replace-color.cjs",
    "storybook": "storybook dev -p 6006",
    "build:sb": "storybook build",
    "deploy:sb": "storybook build && gh-pages -d storybook-static"
  },
